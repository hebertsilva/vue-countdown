{
  "name": "vue-countdown",
  "version": "0.1.0",
  "main": "dist/vue-countdown.common.js",
  "module": "dist/vue-countdown.esm.js",
  "browser": "dist/vue-countdown.js",
  "files": [
    "src",
    "dist"
  ],
  "private": true,
  "scripts": {
    "dev": "rollup -c -m -w",
    "open": "vue-cli-service build --dest public/ public/main.js && vue-cli-service serve --open public/",
    "codecov": "cat coverage/lcov.info | codecov",
    "compress": "uglifyjs ./dist/vue-countdown.js -o ./dist/vue-countdown.min.js -c -m --comments /^!/",
    "build": "rollup -c",
    "release": "npm run lint && npm run build && npm run compress && npm test",
    "lint": "eslint src test *.js --fix",
    "start": "npm run dev",
    "test": "karma start"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "puppeteer": "^1.19.0",
    "rollup-plugin-serve": "^1.0.1",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "change-case": "^3.1.0",
    "codecov": "^3.5.0",
    "create-banner": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "karma": "^4.2.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-rollup-preprocessor": "^7.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "mocha": "^6.2.0",
    "rollup": "^1.10.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-watch": "^4.3.1",
    "vue-template-compiler": "^2.6.10",
    "uglify-js": "^3.6.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
